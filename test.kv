<Level1>:
    name: "level1"
    on_pre_enter: app.screen_on_pre_enter(1)
    on_pre_leave: app.screen_on_pre_leave(1)
    on_enter: app.screen_on_enter(1)
    FloatLayout:
        id: layout_lvl1
        on_touch_down: app.touch_down_handler(1, args)
        canvas.before:
            Rectangle:
                pos: self.pos
                size: self.size
                source: "images/levels-bg/bg_lvl1.jpg"
        NumCollectedCoins:
            id: num_coins_collected_lvl1
        LevelNumber
            id: level_number_lvl1
        RemainingLifePercent
            id: remaining_life_percent_lvl1
        Monster:
            id: monster1_image_lvl1
            monst_anim_duration_low: 3.5
            monst_anim_duration_high: 4.5
        Character:
            id: character_image_lvl1

